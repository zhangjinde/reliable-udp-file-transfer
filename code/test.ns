set ns [new Simulator]                  
source tb_compat.tcl

# Nodes
set goodA [$ns node]
set goodB [$ns node]

set badA [$ns node]
set badB [$ns node]

# Links
set goodLink [$ns duplex-link $goodA $goodB 100Mb 5ms DropTail]
tb-set-link-loss $goodLink 0.01

set badLink [$ns duplex-link $badA $badB 100Mb 100ms DropTail]
tb-set-link-loss $badLink 0.20

# OS   
tb-set-node-os $goodA Ubuntu1604-STD
tb-set-node-os $goodB Ubuntu1604-STD
tb-set-node-os $badA Ubuntu1604-STD
tb-set-node-os $badB Ubuntu1604-STD

$ns rtproto Static
$ns run